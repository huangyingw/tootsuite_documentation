[remote "tootsuite"]
    url = git@github.com:tootsuite/documentation.git
    fetch = +refs/heads/*:refs/remotes/tootsuite/*
[remote "origin"]
    url = git@github.com:huangyingw/tootsuite_documentation.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = tootsuite
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/ossn/tootsuite/documentation
